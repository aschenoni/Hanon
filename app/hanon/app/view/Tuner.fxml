<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="270.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="hanon.app.controller.TunerController">
    <Label layoutX="14.0" layoutY="14.0" text="Listening...">
        <font>
          <Font size="23.0"/>
        </font>
    </Label>
    <GridPane layoutX="44.0" layoutY="113.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="50.0"
              AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="50.0">
      <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
      </columnConstraints>
      <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      </rowConstraints>
        <children>
          <Label text="Note">
              <font>
                  <Font size="20.0"/>
              </font>
          </Label>
          <Label text="Octave" GridPane.columnIndex="1">
              <font>
                  <Font size="20.0"/>
              </font>
          </Label>
          <Label fx:id="noteName" prefHeight="36.0" prefWidth="107.0" GridPane.rowIndex="1">
              <font>
                  <Font size="25.0"/>
              </font>
          </Label>
          <Label fx:id="octave" prefHeight="36.0" prefWidth="56.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
              <font>
                  <Font size="25.0"/>
              </font>
          </Label>
          <Label text="Frequency" GridPane.rowIndex="2">
              <font>
                  <Font size="20.0"/>
              </font>
          </Label>
          <Label text="Difference" GridPane.columnIndex="1" GridPane.rowIndex="2">
              <font>
                  <Font size="20.0"/>
              </font>
          </Label>
          <Label fx:id="difference" prefHeight="36.0" prefWidth="107.0" GridPane.columnIndex="1"
                 GridPane.rowIndex="3">
              <font>
                  <Font size="25.0"/>
              </font>
          </Label>
          <Label fx:id="frequency" prefHeight="36.0" prefWidth="107.0" GridPane.rowIndex="3">
              <font>
                  <Font size="25.0"/>
              </font>
          </Label>
        </children>
    </GridPane>
</AnchorPane>
